{
  "name": "typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "typeorm": "typeorm",
    "migration:create": "cd ./src/database/migrations && typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/index.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/index.ts"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/csv-parse": "^1.2.2",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  }
}
